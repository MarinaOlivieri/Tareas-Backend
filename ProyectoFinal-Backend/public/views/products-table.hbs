<h2>Lista de Productos</h2>
<div>
    {{#if products.length}}
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Stock</th>
            <th>Imagen</th>
        </tr>
        {{#each products}}
        <tr>
            <td>{{this.title}}</td>
            <td>${{this.price}}</td>
            <td>{{this.stock}}</td>
            <td><img width="48" src={{this.thumbnail}} alt="image"></td>
        </tr>
    {{/each}}
    </table>
    {{else}}
    <div>
        No hay productos
    </div>
    {{/if}}
</div>